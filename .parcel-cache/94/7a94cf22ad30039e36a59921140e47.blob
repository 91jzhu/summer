*{box-sizing: border-box;margin:0;padding:0;}
body{
    background-color: rgb(182, 254, 252);
}
.body{
    border:1px solid red;
    height:120px;
    width: 70px;
    background-color: rgb(255, 165, 0);
    margin-left: auto;
    margin-right: auto;
    margin-top: 50px;
    border-radius: 35px;
    position: relative;
}
.ear{
    border:1px solid black;
    height:50px;
    width:30px;
    margin-top: -20px;
}
.ear.left{
    position: absolute;
    border-radius: 200px/300px;
}
.ear.right{
    position: absolute;
    right:0;
    border-radius: 160px 180px 50px 1px;
}
